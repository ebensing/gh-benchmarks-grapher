doctype 5
html(lang="en")
  head
    title=pageTitle
    script(src='http://d3js.org/d3.v3.min.js', charset='utf-8')
    script(src='http://code.jquery.com/jquery-2.0.3.min.js', type='text/javascript')
    script(src='bootstrap/js/bootstrap.min.js', type='text/javascript')
    script(src='graphs.js', type='text/javascript')
    link(rel='stylesheet', type='text/css', href='style.css')
    link(rel='stylesheet', type='text/css', href='bootstrap/css/bootstrap.min.css')
  body
    div.navbar.navbar-inverse.navbar-fixed-top
      div.navbar-inner
        div.container-fluid
          ul.nav
            li
              a(href='#').brand #{projectName}
          ul.nav
            li.dropdown
              a(href='#', data-toggle='dropdown').dropdown-toggle
                | Charts
                b.caret
              ul.dropdown-menu
                each chart, i in charts
                  li
                    a(href='#'+ i) #{chart.title}
    
    div.container-fluid
      div.row-fluid
        div.hero-unit
